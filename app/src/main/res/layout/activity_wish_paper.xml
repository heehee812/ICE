<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WishPaper">

    <EditText
        android:id="@+id/edit_wish_papper"
        android:layout_width="333dp"
        android:layout_height="419dp"
        android:width="3dp"
        android:background="#FFFFFF"
        android:inputType="textMultiLine"
        android:textColor="#A790A5"
        android:visibility="invisible"
        android:gravity="left"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.683"
        tools:ignore="LabelFor,MissingConstraints,RtlHardcoded" />

    <Button
        android:id="@+id/button_wish_done"
        android:layout_width="101dp"
        android:layout_height="33dp"
        android:background="#D1A0E2"
        android:gravity="center"
        android:text="@string/done"
        android:textColor="#E8EAE1"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.874"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.921"
        tools:ignore="MissingConstraints,OnClick" />

    <Button
        android:id="@+id/button_wish_cancel"
        android:layout_width="101dp"
        android:layout_height="33dp"
        android:background="#D1A0E2"
        android:gravity="center"
        android:text="@string/cancel"
        android:textColor="#E8EAE1"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.921"
        tools:ignore="MissingConstraints,OnClick" />


</androidx.constraintlayout.widget.ConstraintLayout>